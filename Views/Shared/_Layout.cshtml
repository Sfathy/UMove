@using Microsoft.AspNet.Identity

@{
    var userInCookie = Request.Cookies["user"];

}

<!DOCTYPE html>
<html dir="@UMoveNew.Views.Shared.Layout.Direction">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>NitroGFX.COM - All For WebMasters... CargoPress</title>
    <!-- Styling -->
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="@UMoveNew.Views.Shared.Layout.style" rel="stylesheet">
    <link href="~/Content/magnific-popup.css" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Roboto%3A400%2C700%7CSource+Sans+Pro%3A700%2C900&amp;subset=latin" rel="stylesheet">

    <script src="~/Scripts/modernizr.custom.24530.js" type="text/javascript"></script>
    <script src='@Url.Content("~/Scripts/jquery.validate.js")' type='text/javascript'></script>
    <script src='@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")' type='text/javascript'></script>
    <link rel="shortcut icon" href="~/images/fav.png">
    @RenderSection("Styles", false)


</head>
<body>
    <div class="boxed-container">

        <!-- TOP BAR -->
        <div class="top">



            <div class="container">

                <div class="top__tagline">
                    <div class="widget-icon-box">
                        <div class="collapse navbar-collapse" style="width: 100%; height: 100%; background-color: #1f425d;">
                            @Html.Partial("_LoginPartial")
                        </div>
                    </div>
                </div>



            </div><!-- /.container -->

        </div> <!-- /.top -->
        <div class="header__container">

            <div class="container">

                <header class="header">

                    <div class="header__logo">
                        <a href="index.html">
                            <img class="img-responsive" srcset="~/images/logo.png" alt="CargoPress" src="images/logo.png">
                        </a>
                        <button data-target="#cargopress-navbar-collapse" data-toggle="collapse" class="navbar-toggle" type="button">
                            <span class="navbar-toggle__text">@UMoveNew.Views.Shared.Layout.MENU</span>
                            <span class="navbar-toggle__icon-bar">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </span>
                        </button>
                    </div><!-- /.header__logo -->

                    <div class="header__navigation">
                        <nav class="collapse navbar-collapse" id="cargopress-navbar-collapse">
                            <ul class="main-navigation js-main-nav js-dropdown">
                                <li >
                                    <a href="~/">@UMoveNew.Views.Shared.Layout.Home</a>
                                   
                                </li>
                              
                                @if (userInCookie != null)
                                {
                                <li><a href="~/Trips/Categories">@UMoveNew.Views.Shared.Layout.NewShipment</a></li>
                                    <li><a href="~/Trips/AllShipments">@UMoveNew.Views.Shared.Layout.FindShipment</a></li>
                                }
                            </ul>

                        </nav>
                    </div><!-- /.header__navigation -->

                    <div class="header__widgets">

                        <div class="widget-icon-box">

                            <div class="icon-box">
                                <i class="fa fa-headphones"></i>
                                <h4 class="icon-box__title">@UMoveNew.Views.Shared.Layout.CallUsAnytime</h4>
                                <span class="icon-box__subtitle">1-888-123-4567</span>
                            </div>

                        </div>

                        <div class="widget-icon-box">

                            <div class="icon-box">
                                <i class="fa fa-clock-o"></i>
                                <h4 class="icon-box__title">@UMoveNew.Views.Shared.Layout.OpeningTime</h4>
                                <span class="icon-box__subtitle">08:00 - 18:00</span>
                            </div>

                        </div>

                        <div class="widget-icon-box">

                            <div class="icon-box">
                                <i class="fa fa-envelope-o"></i>
                                <h4 class="icon-box__title">@UMoveNew.Views.Shared.Layout.EmailUs</h4>
                                <span class="icon-box__subtitle">info@UMove.com</span>
                            </div>

                        </div>

                        @*<a target="_self" href="#" class="btn btn-info" id="button_requestQuote" style="width:100%;height:100%">REQUEST A QUOTE</a>*@

                    </div><!-- /.header__widgets -->

                    <div class="header__navigation-widgets">
                        <a target="_blank" href="https://www.facebook.com/" class="social-icons__link"><i class="fa fa-facebook"></i></a>
                        <a target="_blank" href="https://twitter.com/" class="social-icons__link"><i class="fa fa-twitter"></i></a>
                        <a target="_blank" href="#" class="social-icons__link"><i class="fa fa-wordpress"></i></a>
                        <a target="_blank" href="https://www.youtube.com/" class="social-icons__link"><i class="fa fa-youtube"></i></a>
                    </div>

                </header>

            </div><!-- /.container -->

        </div><!-- /.header__container -->

        <div>
            @RenderBody()
        </div>
        <footer class="footer">

            <div class="footer-top">

                <div class="container">

                    <div class="row">

                        <div class="col-xs-12 col-md-4">

                            <p>
                                <img alt="logo-footer" src="~/images/logo-footer.png" class="logo">
                            </p>
                           
                            <p>
                                <a target="_blank" href="https://www.facebook.com/" class="icon-container icon-container--square">
                                    <span class="fa fa-facebook"></span>
                                </a>
                                <a target="_blank" href="https://twitter.com/" class="icon-container icon-container--square">
                                    <span class="fa fa-twitter"></span>
                                </a>
                                <a target="_blank" href="#" class="icon-container icon-container--square">
                                    <span class="fa fa-wordpress"></span>
                                </a>
                                <a target="_blank" href="https://www.youtube.com/" class="icon-container icon-container--square">
                                    <span class="fa fa-youtube-play"></span>
                                </a>
                            </p>

                        </div><!-- /.row -->

                        <div class="col-xs-12 col-md-2">

                            <div class="widget_nav_menu">
                                <h6 class="footer-top__headings">@UMoveNew.Views.Shared.Layout.NAVIGATION</h6>
                                <ul>
                                    <li>
                                        <a href="tables.html">@UMoveNew.Views.Shared.Layout.Extras</a>
                                        <ul class="sub-menu">
                                            <li>
                                                <a href="tables.html">@UMoveNew.Views.Shared.Layout.Tables</a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a href="#">@UMoveNew.Views.Shared.Layout.BuyNow</a>
                                    </li>
                                    <li>
                                        <a href="#" target="_blank">@UMoveNew.Views.Shared.Layout.ThemeDocumentation</a>
                                    </li>
                                </ul>
                            </div><!-- /.widget_nav_menu -->

                        </div><!-- /.row -->
                        <div class="col-xs-12 col-md-2">




                        </div><!-- /.row -->

                        <div class="col-xs-12 col-md-2">

                            <div class="widget_nav_menu">

                                <h6 class="footer-top__headings">@UMoveNew.Views.Shared.Layout.OURSERVICES</h6>
                                <ul>
                                    <li>
                                        <a href="services.html">@UMoveNew.Views.Shared.Layout.AllServices</a>
                                    </li>
                                    <li>
                                        <a href="ground_transport.html">@UMoveNew.Views.Shared.Layout.GroundTransport</a>
                                    </li>
                                    <li>
                                        <a href="cargo.html">@UMoveNew.Views.Shared.Layout.Umove</a>
                                    </li>
                                    <li>
                                        <a href="warehousing.html">@UMoveNew.Views.Shared.Layout.Warehousing</a>
                                    </li>
                                    <li>
                                        <a href="logistic_service.html">@UMoveNew.Views.Shared.Layout.LogisticService</a>
                                    </li>
                                    <li>
                                        <a href="trucking_service.html">@UMoveNew.Views.Shared.Layout.TruckingService</a>
                                    </li>
                                    <li>
                                        <a href="storage.html">@UMoveNew.Views.Shared.Layout.Storage</a>
                                    </li>
                                </ul>

                            </div><!-- /.widget_nav_menu -->

                        </div><!-- /.row -->

                      
                    </div><!-- /.row -->

                </div><!-- /.footer -->

            </div><!-- /.footer-top -->

            <div class="footer-bottom">

                <div class="container">

                    <div class="footer-bottom__left">
                        Theme Made by Hossam Fathy.
                    </div>

                    <div class="footer-bottom__right">
                        Copyright &copy; 2016&ndash;2017 UMove. All rights reserved.
                    </div>

                </div><!-- /.container -->

            </div><!-- /.footer-bottom -->

        </footer>

    </div>
    <script src="~/Scripts/jquery-2.1.4.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap/carousel.js"></script>
    <script src="~/Scripts/bootstrap/transition.js"></script>
    <script src="~/Scripts/bootstrap/button.js"></script>
    <script src="~/Scripts/bootstrap/collapse.js"></script>
    <script src="~/Scripts/bootstrap/validator.js"></script>
    <script src="~/Scripts/underscore.js"></script>
    <script src="https://maps.google.com/maps/api/js?sensor=false"></script>
    <script src="~/Scripts/SimpleMap.js"></script>
    <script src="~/Scripts/NumberCounter.js"></script>
    <script src="~/Scripts/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/custom.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
